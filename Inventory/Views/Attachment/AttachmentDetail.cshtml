@model Inventory.Models.AttachmentViewModel
@{
    ViewBag.Title = "Photo Page";
}
 
<h4>@Model.AttachmentMakeModel #@Model.AttachmentNum</h4>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <img src="@Html.DisplayFor(m => m.Photo)" height="400" />
        </div>
    </div>
    <br />
    <br />
    <div class="row">
        <div class="col-md-12">
            @Html.ActionLink("Return to Attachment List", "Index", new { @class = "btn btn-primary" })
        </div>
    </div>
</div>